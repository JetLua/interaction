{
  "presets": [
    ["@babel/env", {"useBuiltIns": "usage", "corejs": 3, "modules": false}]
  ],

  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-export-namespace-from",
    ["@babel/transform-runtime", {
      "corejs": 3,
      "helpers": false,
      "regenerator": true,
      "useESModules": true
    }]
  ]
}